    "build": "tsc",
    "format": "prettier --write .",
    "release": "np --no-tests --no-publish"
  },
  "dependencies": {
    "@auth/core": "^0.38.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.11",
    "@vitest/coverage-v8": "^3.0.7",
    "mongoose": "^8.3.4",
    "np": "^10.0.5",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vitest": "^3.0.7"
  },
  "peerDependencies": {
    "mongoose": "^8"
  }
